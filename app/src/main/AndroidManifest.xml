<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.ns.stellarjet">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_stellar_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_stellar_launcher"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".drawer.PreferenceLaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".home.HomeActivity"/>
        <activity android:name=".login.LoginActivity"/>
        <activity android:name=".login.PasswordActivity"/>
        <activity android:name=".login.OTPActivity"/>
        <activity android:name=".authenticate.AuthenticationActivity"/>
        <activity android:name=".booking.PlaceSelectionActivity"/>
        <activity android:name=".booking.CalendarActivity"/>
        <activity android:name=".booking.PassengerActivity"/>
        <activity android:name=".booking.BookingConfirmedActivity"/>
        <activity android:name=".personalize.PersonalizeLaunchActivity"/>
        <activity android:name=".personalize.CabPreferencesActivity"/>
        <activity android:name=".personalize.SavedAddressActivity"/>
        <activity android:name=".personalize.AddAddressActivity"/>
        <activity android:name=".personalize.FoodPreferencesLaunchActivity"/>
        <activity android:name=".personalize.FoodPreferenceListActivity"/>
        <activity android:name=".personalize.PersonalizeSuccessActivity"/>
        <activity android:name=".drawer.DrawerActivity"/>
        <activity android:name=".drawer.MyBookingsActivity"/>
        <activity android:name=".drawer.BookingsDetailsActivity"/>
        <activity android:name=".drawer.BoardingPassActivity"/>
        <activity android:name=".drawer.BoardingPassDetailsActivity"/>
        <!-- <activity android:name=".drawer.PreferenceLaunchActivity"/> -->
        <activity android:name=".drawer.PreferencesManagersAddActivity">
        </activity>
    </application>

</manifest>